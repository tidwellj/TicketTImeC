<Window x:Name="MessageBoxOK" x:Class="TicketTime.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TicketTime"
        mc:Ignorable="d"
        Title="" Height="300" Width="500" ResizeMode="NoResize" IsEnabled="False">
    <Grid>
        <Border BorderBrush="Black" BorderThickness="1">
            <Button x:Name="Enter" Content="OK" HorizontalAlignment="Center" Width="88" Height="30" VerticalAlignment="Center" Background="#FFEFEFF7" Foreground="#FF0E0D0D" BorderBrush="#FF252526" Click="Set_Click" Margin="0,180,0,0">
                <Button.Resources>
                    <Style TargetType="{x:Type Border}">
                        <Setter Property="CornerRadius" Value="5" />
                    </Style>
                </Button.Resources>
            </Button>
        </Border>

    </Grid>
</Window>
